{
  "name": "openmemory-plus",
  "version": "1.2.3",
  "description": "Agent Memory Management CLI - Install, configure, and manage OpenMemory Plus",
  "type": "module",
  "bin": {
    "openmemory-plus": "dist/index.js",
    "omp": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "ai",
    "agent",
    "memory",
    "openmemory",
    "cli",
    "qdrant",
    "ollama",
    "mcp",
    "llm",
    "claude",
    "cursor",
    "augment"
  ],
  "author": "TreeRen Chou <miaojsi@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alenryuichi/openmemory-plus.git"
  },
  "bugs": {
    "url": "https://github.com/Alenryuichi/openmemory-plus/issues"
  },
  "homepage": "https://github.com/Alenryuichi/openmemory-plus#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "inquirer": "^9.2.0",
    "ora": "^8.0.0",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.0.0",
    "@types/which": "^3.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "templates"
  ]
}
